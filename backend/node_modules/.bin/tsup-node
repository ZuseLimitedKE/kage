#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/dist/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/dist/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules:/home/mbeka02/Desktop/kage/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/dist/cli-node.js" "$@"
else
  exec node  "$basedir/../.pnpm/tsup@8.5.0_tsx@4.20.3_typescript@5.9.2/node_modules/tsup/dist/cli-node.js" "$@"
fi
